</ndl:Interface>

<ndl:Interface rdf:about="#router:R-AS2-1:lo0.1">
        <ndl:name>router:R-AS2-1:lo0.1</ndl:name>
</ndl:Interface>

<ndl:Interface rdf:about="#router:R-AS2-2:lt-1/2/0.101">

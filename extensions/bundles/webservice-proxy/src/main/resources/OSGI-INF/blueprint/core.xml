    <jaxws:client id="netOSPFCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.INetOSPFCapabilityService" address="http://0.0.0.0:8182/cxf/netOSPFCapabilityService" />
    <service ref="netOSPFCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.INetOSPFCapabilityService" />

    <!-- Net Queue Capability -->
    <jaxws:client id="netQueueCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.INetQueueCapabilityService" address="http://0.0.0.0:8182/cxf/netQueueCapabilityService" />
    <service ref="netQueueCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.INetQueueCapabilityService" />

    <!-- Protocol Session Manager -->
    <jaxws:client id="protocolSessionManagerServiceProxy" serviceClass="org.opennaas.extensions.ws.services.IProtocolSessionManagerService" address="http://0.0.0.0:8182/cxf/protocolSessionManagerService" />
    <service ref="protocolSessionManagerServiceProxy" interface="org.opennaas.extensions.ws.services.IProtocolSessionManagerService" />
    
</blueprint>
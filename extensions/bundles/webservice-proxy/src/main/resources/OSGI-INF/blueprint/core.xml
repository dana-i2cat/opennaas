    <jaxws:client id="ipCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.IIPCapabilityService" address="http://0.0.0.0:8182/cxf/ipCapabilityService" />
    <service ref="ipCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.IIPCapabilityService" />

    <!-- NetworkBasic Capability -->
    <jaxws:client id="networkBasicCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.INetworkBasicCapabilityService" address="http://0.0.0.0:8182/cxf/networkBasicCapabilityService" />
    <service ref="networkBasicCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.INetworkBasicCapabilityService" />


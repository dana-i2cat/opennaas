    <jaxws:client id="queueManagerCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.IQueueManagerCapabilityService" address="http://0.0.0.0:8182/cxf/queueManagerCapabilityService" />
    <service ref="queueManagerCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.IQueueManagerCapabilityService" />


    <jaxws:client id="staticRouteCapabilityServiceProxy" serviceClass="org.opennaas.extensions.ws.services.IStaticRouteCapabilityService" address="http://0.0.0.0:8182/cxf/staticRouteCapabilityService" />
    <service ref="staticRouteCapabilityServiceProxy" interface="org.opennaas.extensions.ws.services.IStaticRouteCapabilityService" />

